num=int(input("Enter a number:"))
if(num==1):
    print("SUNDAY")
elif(num==2):
    print("MONDAY")
elif(num==3):
    print("TUESDAY")
elif(num==4):
    print("WEDNESDAY")
elif(num==5):
    print("THURSDAY")
elif(num==6):
    print("FRIDAY")
elif(num==7):
    print("SATURDAY")
else:
    print("Invalid Input")
